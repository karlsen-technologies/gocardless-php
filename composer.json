    "description": "PHP package for using the GoCardless API",
    "autoload": {
        "psr-4": {
            "KarlsenTechnologies\\GoCardless\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Thomas Andre Karlsen",
            "email": "thomas@kartech.no"
        }
    ],
    "require-dev": {
